{
  "buildCommand": "cd ../.. && npm install && npx turbo build --filter=api",
  "outputDirectory": "dist",
  "devCommand": "npm run start:dev",
  "functions": {
    "api/index.ts": {
      "includeFiles": "dist/**"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ]
}